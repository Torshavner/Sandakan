{
  "server": {
    "host": "0.0.0.0",
    "port": 8080
  },
  "qdrant": {
    "url": "http://qdrant:6334",
    "collection_name": "rag_chunks"
  },
  "embeddings": {
    "model": "text-embedding-3-large",
    "strategy": "semantic",
    "dimension": 3072,
    "chunk_overlap": 100
  },
  "chunking": {
    "max_chunk_size": 1024,
    "overlap_tokens": 100
  },
  "llm": {
    "api_key": "",
    "chat_model": "gpt-4o",
    "max_tokens": 8192,
    "temperature": 0.5
  },
  "logging": {
    "level": "warn",
    "enable_json": true,
    "enable_udp": false
  },
  "extraction": {
    "pdf": {
      "enabled": true,
      "max_file_size_mb": 100
    },
    "audio": {
      "enabled": true,
      "max_file_size_mb": 200,
      "whisper_model": "medium"
    }
  }
}
