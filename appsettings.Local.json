{
  "server": {
    "host": "127.0.0.1",
    "port": 3000
  },
  "qdrant": {
    "url": "http://localhost:6334",
    "collection_name": "rag_chunks"
  },
  "embeddings": {
    "model": "text-embedding-3-small",
    "strategy": "semantic",
    "dimension": 1536,
    "chunk_overlap": 50
  },
  "chunking": {
    "max_chunk_size": 512,
    "overlap_tokens": 50
  },
  "llm": {
    "api_key": "",
    "chat_model": "gpt-4o-mini",
    "max_tokens": 4096,
    "temperature": 0.7
  },
  "logging": {
    "level": "debug",
    "enable_json": false,
    "enable_udp": true
  },
  "extraction": {
    "pdf": {
      "enabled": true,
      "max_file_size_mb": 50
    },
    "audio": {
      "enabled": true,
      "max_file_size_mb": 100,
      "whisper_model": "base"
    }
  }
}
